{
    "name": "vscode-jxpath",
    "displayName": "JXpath",
    "description": "",
    "version": "0.0.1",
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/jianzhichun/vscode-jxpath.git"
    },
    "publisher": "joseph-chun",
    "engines": {
        "vscode": "^1.0.0"
    },
    "icon": "img/j.jpg",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "keybindings": [{
            "command": "extension.jsonpath",
            "key": "alt+j",
            "when": "editorTextFocus"
        }],
        "commands": [{
            "command": "extension.openjsonpath",
            "title": "Open Jsonpath Search"
        }, {
            "command": "extension.closejsonpath",
            "title": "Close Jsonpath Search"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    },
    "dependencies": {
        "jsonpath": "^0.2.7",
        "xmldom": "^0.1.22",
        "xpath": "0.0.23"
    }
}