{
  "name": "vscode-jxpath",
  "displayName": "JXpath",
  "description": "",
  "version": "0.0.1",
  "publisher": "Joseph-chun",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
